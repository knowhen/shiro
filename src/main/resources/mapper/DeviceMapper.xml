<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.when.shiro.mapper.DeviceMapper">

    <select id="queryByDeviceId" resultType="com.when.shiro.entity.DeviceEntity">
        SELECT * FROM yrs_device WHERE device_id = #{deviceId}
    </select>

    <select id="queryDevices" resultType="com.when.shiro.entity.DeviceEntity">
        SELECT * FROM yrs_device
    </select>

</mapper>